<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
    
    <!-- ======================= Definciones Form Bean  ==================== -->
    <form-beans> 
        <form-bean name="LoginAuxForm" type="pe.com.gp.form.LoginForm" ></form-bean>
        <form-bean name="GenericoForm" type="pe.com.gp.form.GenericoForm"></form-bean>       
        <form-bean name="PedidoOfertaRepForm" type="pe.com.gp.form.PedidoOfertaRepForm"></form-bean>  
        <form-bean name="StockForm" type="pe.com.gp.form.StockForm"></form-bean>  
        <form-bean name="CatalogoForm" type="pe.com.gp.form.CatalogoForm"></form-bean>
        <form-bean name="ProductosForm" type="pe.com.gp.form.ProductosForm"></form-bean>  
        <form-bean name="ImpresionForm" type="pe.com.gp.form.ImpresionForm"></form-bean>  
        <form-bean name="TrasladoForm" type="pe.com.gp.form.TrasladoForm"></form-bean>                      
        <form-bean name="RegistroVentasForm" type="pe.com.gp.form.RegistroVentasForm"></form-bean> 
        <form-bean name="HelpForm" type="pe.com.gp.form.HelpForm"></form-bean>
        <form-bean name="ComprasMigrarForm" type="pe.com.gp.form.ComprasMigrarForm"></form-bean>
        <form-bean name="PreProvisionForm" type="pe.com.gp.form.PreProvisionForm"></form-bean>
        
        <form-bean name="ClientesForm" type="pe.com.gp.form.ClientesForm"></form-bean>
        <form-bean name="ProgramaPagosForm" type="pe.com.gp.form.PreProvisionForm"></form-bean>  
        
        <form-bean name="ProgramaPagosPolizaForm" type="pe.com.gp.form.PreProvisionForm"></form-bean>
        
        <form-bean name="ResponsableMailForm" type="pe.com.gp.form.ResponsableMailForm"></form-bean> 
        <form-bean name="PerfilForm" type="pe.com.gp.form.PerfilForm"></form-bean>  
        <form-bean name="ArchivoBancoForm" type="pe.com.gp.form.ArchivoBancoForm"></form-bean>
          
    </form-beans>
    
    <!-- ======================= Definiciones Action  ====================== -->
    <global-forwards>
        <forward name="logout" path="/relogin.jsp"/>
        <forward name="error" path="/error.jsp"/>
    </global-forwards>

    <action-mappings>                
        <!-- 
            - name      : nombre del form-bean
            - path      : invocar desde la vista
            - type      : nombre de la clase
            - parameter : nombre del metodo en la clase
            - forward   : redireccion 
            - scope     : ambito
        -->
               
        <!-- =============================================================== -->
        <!-- L O G I N -->
        <!-- =============================================================== -->
        <action name="LoginAuxForm" scope="request" path="/LoginAuxiliarAction" type="pe.com.gp.action.LoginAuxiliarAction" parameter="operacion">            
            <forward name="Teclado" path="/wpTeclado.jsp" />
            <forward name="mnuMain" path="/index.jsp" />
            <forward name="validaIngreso" path="/Teclado.jsp" />
          
          
            <!--<forward name="aprLiqOtEspecial" path="/AprobacionLiqOtEspAction.do?operacion=inicializa"></forward>
            <forward name="aprOrdSalOT" path="/AprobacionOrdenSalidaOT.do?operacion=inicializa"></forward>-->
        </action>
        
        <!-- =============================================================== -->
        <!-- A J A X -->
        <!-- =============================================================== -->
        <!--<action name="GenericoForm" scope="request" path="/AjaxAction" type="pe.com.gp.action.AjaxAction" parameter="operacion"/>-->
                 
       

        <!--<action name="FacRepMostradorForm" scope="request" path="/PedidoOfertaRepAction" type="pe.com.gp.action.PedidoOfertaRepAction" parameter="operacion">
            <forward name="inicializa" path="/consultaEntregaPedidos.jsp"></forward>            
        </action>-->
        <action name="PedidoOfertaRepForm" scope="request" path="/PedidoOfertaRepAction" type="pe.com.gp.action.PedidoOfertaRepAction" parameter="operacion">
            <forward name="recibirCotizacion" path="/pedidoRepuestos.jsp"></forward>
            <forward name="msgConFacRepMostrador" path="/pedidoRepuestos.jsp"></forward>
            <forward name="inicializa" path="/pedidoRepuestos.jsp"></forward>
            <forward name="inicializaCot" path="/pedidoRepuestos.jsp"></forward>
            
            
        </action>
        
        <action name="StockForm" scope="request" path="/StockAction" type="pe.com.gp.action.StockAction" parameter="operacion">
            <forward name="inicializa" path="/Stock.jsp"></forward>
        </action>

        <action name="StockForm" scope="request" path="/KardexAction" type="pe.com.gp.action.KardexAction" parameter="operacion">
            <forward name="inicializa" path="/kardex.jsp"></forward>
        </action>

        <action name="StockForm" scope="request" path="/BoletaAlmacenAction" type="pe.com.gp.action.BoletaAlmacenAction" parameter="operacion">
            <forward name="inicializa" path="/boletasAlmacen.jsp"></forward>
        </action>                        

        <action name="ProductosForm" scope="request" path="/ProductosAction" type="pe.com.gp.action.ProductosAction" parameter="operacion">
            <forward name="inicializa" path="/Productos.jsp"></forward>
        </action>   
        
        <action name="CatalogoForm" scope="request" path="/CatalogoAction" type="pe.com.gp.action.CatalogoAction" parameter="operacion">
            <forward name="inicializa" path="/catalogo.jsp"></forward>
        </action>  
        
        <action name="ImpresionForm" scope="request" path="/ImpresionAction" type="pe.com.gp.action.ImpresionAction" parameter="operacion">
            <forward name="inicializa" path="/impresionDocumentos.jsp"></forward>
        </action>          
        
                    
        <action name="TrasladoForm" scope="request" path="/TrasladoAction" type="pe.com.gp.action.TrasladoAction" parameter="operacion">
            <forward name="inicializa" path="/traslados.jsp"></forward>
        </action>                                               

        <action name="RegistroVentasForm" scope="request" path="/RegistroVentasAction" type="pe.com.gp.action.RegistroVentasAction" parameter="operacion">
            <forward name="inicializa" path="/registroVentas.jsp"></forward>
            <forward name="inicializaMigrar" path="/registroVentasMigrar.jsp"></forward>
            <forward name="msgRegistroVentas" path="/registroVentasMigrar.jsp"></forward>
        </action>         
           
        <action name="TrasladoForm" scope="request" path="/ControlCajaAction" type="pe.com.gp.action.ControlCajaAction" parameter="operacion">
            <forward name="inicializa" path="/controlCajas.jsp"></forward>
            <forward name="msgConControlCajas" path="/controlCajas.jsp"></forward>
        </action>                                      


        <action name="HelpForm" scope="request" path="/HelpAction" type="pe.com.gp.action.HelpAction" parameter="operacion">
            <forward name="inicializa" path="/help.jsp"></forward>
        </action>                                                                   
      
        <action name="ComprasMigrarForm" scope="request" path="/ComprasMigrarAction" type="pe.com.gp.action.ComprasMigrarAction" parameter="operacion">
            <forward name="inicializa" path="/ComprarMigrar.jsp"></forward>
        </action>  
        
        <action name="PreProvisionForm" scope="request" path="/PreProvisionAction" type="pe.com.gp.action.PreProvisionAction" parameter="operacion">
            <forward name="inicializa" path="/ListaPreProvision.jsp"></forward>
        </action>          
        <action name="ClientesForm" scope="request" path="/ProveedorAction" type="pe.com.gp.action.ProveedorAction" parameter="operacion">
            <forward name="inicializa" path="/proveedores.jsp"></forward>
            
             <forward name="inicializaCuentaEmpresa" path="/cuentasEmpresa.jsp"></forward>
        </action>         
                    
        <action name="ProgramaPagosForm" scope="request" path="/ProgramaPagosAction" type="pe.com.gp.action.ProgramaPagosAction" parameter="operacion">
            <forward name="inicializa" path="/programaPagos.jsp"></forward>
            <forward name="grabaPagoProveedor" path="/programaPagos.jsp"></forward>
            <forward name="msgGrabaPagoProveedor" path="/programaPagos.jsp"></forward>
        </action>    
        
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                          
        <action name="ProgramaPagosForm" scope="request" path="/ProgramaPagosPolizaAction" type="pe.com.gp.action.ProgramaPagosPolizaAction" parameter="operacion">
            <!-- -<forward name="inicializa" path="/programaPagos.jsp"></forward> 
            <forward name="grabaPagoProveedor" path="/programaPagos.jsp"></forward>
            <forward name="msgGrabaPagoProveedor" path="/programaPagos.jsp"></forward>             
            -->
            <forward name="inicializa" path="/programaPagosPoliza.jsp"></forward>  
            <forward name="grabaPagoProveedorPoliza" path="/programaPagosPoliza.jsp"></forward>  
            <forward name="msgGrabaPagoPoliza" path="/programaPagosPoliza.jsp"></forward> 
                            
        </action>                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                
                     
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    
        <action name="ProgramaPagosForm" scope="request" path="/fechaProgramacionDocAction" type="pe.com.gp.action.fechaProgramacionDocAction" parameter="operacion">
           
            <forward name="inicializa" path="/fechaProgramacionDoc.jsp"></forward>
            <forward name="grabaProgramaciondePago" path="/fechaProgramacionDoc.jsp"></forward>
            <forward name="msgProgramaciondePago" path="/fechaProgramacionDoc.jsp"></forward>
            
            
           
            <forward name="inicializaRango" path="/programacionRango.jsp"></forward>
            <forward name="msgGrabaRango" path="/programacionRango.jsp"></forward>
        </action>                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                    
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                  
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                
        <action name="ResponsableMailForm" scope="request" path="/responsablesMailAction" type="pe.com.gp.action.responsablesMailAction" parameter="operacion">
            <forward name="inicializa" path="/MailResponsables.jsp"></forward>
        </action>         
        
        <action name="PerfilForm" scope="request" path="/PerfilAction" type="pe.com.gp.action.PerfilAction" parameter="operacion">
            <forward name="inicializa" path="/crearPerfil.jsp"></forward>
            <forward name="inicializaAsignar" path="/usuarioPerfil.jsp"></forward>
        </action>
        
        
        <action name="ArchivoBancoForm" scope="request" path="/ArchivoBancoAction" type="pe.com.gp.action.ArchivoBancoAction" parameter="operacion">
            <forward name="inicializa" path="/archivoBanco.jsp"></forward>
            <forward name="inicializaConsulta" path="/ConsultaPlanilla.jsp"></forward>
        </action>        
        
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                            
    </action-mappings>    
    <message-resources parameter="pe.com.gp.recursos.Recursos"></message-resources>
</struts-config>